{
    "builds": [
        {
            "src": "agrispeak/agrispeak/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "agrispeak/agrispeak/wsgi.py"
        }
    ]
}
