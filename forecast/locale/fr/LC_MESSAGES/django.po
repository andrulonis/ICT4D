msgstr "Faible"

#: forecast/views.py:22
msgid "Moderate"
msgstr "Modérée"

#: forecast/views.py:23
msgid "Heavy"
msgstr "Forte"

#: forecast/views.py:24
msgid "Very heavy"
msgstr "Très forte"

#: forecast/views.py:25
msgid "Extreme"
msgstr "Extrême"

#: forecast/views.py:26
msgid "Very extreme"
msgstr "Très extrême"

